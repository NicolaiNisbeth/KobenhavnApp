<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <TextView
        android:id="@+id/indstillinger_item_left"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="E-mail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <TextView
        android:id="@+id/indstillinger_item_middle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nicolai.nisbeth@hotmail.com"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toStartOf="@id/indstillinger_item_right"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <ImageButton
        android:id="@+id/indstillinger_item_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/ic_arrow_right"
        />



</androidx.constraintlayout.widget.ConstraintLayout>